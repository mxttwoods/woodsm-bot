{
  "name": "woodsm",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "prestart": "uglifyjs src/main.js -m -c -o src/index.js",
    "start": "node .",
    "format": "npx prettier --write ."
  },
  "dependencies": {
    "discord.js": "12.5.2",
    "dotenv": "8.2.0",
    "mongoose": "5.12.3",
    "node-fetch": "2.6.1",
    "uglify-js": "3.13.3"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always",
    "jsxSingleQuote": true,
    "quoteProps": "consistent"
  }
}
